  },
  "keywords": [
    "Bakhaow",
    "Launcher",
    "Falazia",
    "Minecraft"
  ],
  "author": "Bakhaow",
  "license": "UNLICENSED",
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^22.7.0"
  }
}
